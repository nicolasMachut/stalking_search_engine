            </div>
            <div class="progress" id="searchProgresse" style="display : none;">
                <div class="indeterminate"></div>
            </div>
        </form>
    </div>
    <div id="results" style="display : none;"></div>
</div>
<script>
    $(document).ready(function(){
        $('.tooltipped').tooltip({delay: 50});
    });
</script>